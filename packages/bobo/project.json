{
  "name": "bobo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/bobo/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/bobo",
        "main": "packages/bobo/src/index.ts",
        "tsConfig": "packages/bobo/tsconfig.lib.json",
        "assets": ["packages/bobo/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/bobo/**/*.ts",
          "packages/bobo/package.json"
        ]
      }
    },
    "release": {
      "executor": "nx-release:build-update-publish",
      "options": {
        "libName": "bobo"
      }
    }
  },
  "tags": []
}
